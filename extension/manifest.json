{
  "manifest_version": 2,
  "name": "Link Formatter",
  "description": "Link Formatter - A browser extension to format and copy links.",
  "homepage_url": "https://github.com/ryandav/link-formatter",
  "version": "3.3",
  "browser_action": {
   "default_popup": "popup.html",
   "default_title": "Link Formatter",
   "default_icon": {
      "32": "icons/icon32.png",
      "64": "icons/icon64.png"
   }
  },
  "permissions": [
    "tabs"
  ]
}
